/* pages/test/test.wxss */
.body{
  width:400rpx;
  height:400rpx;
  margin:10rpx auto;
}
.img{
  height:200rpx;
  width:200rpx;
  border-radius: 100rpx;
  animation: sway 10s ease-in-out infinite;
}
@keyframes sway{
  0% {transform: translate3d(0,20rpx,0) rotate(-15deg); }
  17% {transform: translate3d(0,0rpx,0) rotate(25deg); }
  34% {transform: translate3d(0,-20rpx,0) rotate(-20deg); }
  50% {transform: translate3d(0,-10rpx,0) rotate(15deg); }
  67% {transform: translate3d(0,10rpx,0) rotate(-25deg); }
  84% {transform: translate3d(0,15rpx,0) rotate(15deg); }
  100% {transform: translate3d(0,20rpx,0) rotate(-15deg); }
}

.myFirst{
  height:200rpx;
  width:200rpx;
  border-radius: 100rpx;
  background-color:green;
  position: relative;
  animation: myfirst 3s ease-in-out infinite;
}

@keyframes myfirst
{
    0%   {background: red;height:150rpx;width:150rpx;}
    25%  {background: yellow; height:100rpx;width:100rpx;}
    50%  {background: blue;height:50rpx;width:50rpx;}
    75%  {background: green;height:100rpx;width:100rpx;}
    100% {background: red;height:200rpx;width:200rpx;}
}

.box{
  height:200rpx;
  width:200rpx;
  background-color:#ccc;
  position: relative;
  animation: box 10s linear infinite;
}

@keyframes box{
    0% {transform: rotate(0deg); top:100px;}
    5% {transform: rotate(90deg);top:150px; }
    10% {transform: rotate(180deg);top:200px; }
    15% {transform: rotate(270deg);top:250px; }
    20% {transform: rotate(360deg);top:300px; }
    25% {transform: rotate(450deg); top:280px;}
    30% {transform: rotate(540deg);top:275px; }
    35% {transform: rotate(630deg);top:250px; }
    40% {transform: rotate(720deg);top:240px; }
    45% {transform: rotate(810deg);top:260px; }
    50% {transform: rotate(900deg); top:290px;}
    55% {transform: rotate(990deg);top:300px; }
    60% {transform: rotate(1080deg);top:280px; }
    65% {transform: rotate(1170deg);top:270px; }
    70% {transform: rotate(1260deg);top:260px; }
    75% {transform: rotate(1350deg); top:255px;}
    80% {transform: rotate(1440deg);top:270px; }
    85% {transform: rotate(1530deg);top:290px; }
    90% {transform: rotate(1620deg);top:300px; }
    95% {transform: rotate(1710deg);top:280px; }
    100% {transform: rotate(1800deg); top:300px;}
    

}